def is_palindrome(number):
  number_string = str(number)
  return number_string == number_string[::-1]

def find_largest_palindrome(min_number, max_number):
  largest_palindrome = 0
  for i in range(min_number, max_number + 1):
    for j in range(i, max_number + 1):
      product = i * j
      if is_palindrome(product) and product > largest_palindrome:
        largest_palindrome = product
  return largest_palindrome

if __name__ == "__main__":
  min_number = 100
  max_number = 999
  largest_palindrome = find_largest_palindrome(min_number, max_number)
  with open("102-result", "w") as f:
    f.write(str(largest_palindrome))
